<?xml version="1.0" encoding="utf-8"?>
<appengine-web-app xmlns="http://appengine.google.com/ns/1.0">
    <application>progettomarchetto</application>
    <version>1</version>
    <runtime>java8</runtime>
    <threadsafe>true</threadsafe>
    <system-properties>
        <property name="java.util.logging.config.file" value="WEB-INF/logging.properties"/>
    </system-properties>

    <static-files>
        <include path="/**.html"/>
        <include path="/**.js"/>
        <include path="/**.css"/>
        <include path="/**.png"/>
        <include path="/**.yaml"/>
    </static-files>
</appengine-web-app>
